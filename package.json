{
  "name": "agentuse",
  "version": "0.8.0",
  "description": "AI agents on autopilot - define in markdown, run on cron, CI/CD, or serverless",
  "type": "module",
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentuse/agentuse.git"
  },
  "bugs": {
    "url": "https://github.com/agentuse/agentuse/issues"
  },
  "homepage": "https://github.com/agentuse/agentuse#readme",
  "keywords": [
    "ai",
    "agent",
    "mcp",
    "anthropic",
    "openai",
    "cli",
    "llm",
    "automation",
    "cron",
    "cicd",
    "serverless",
    "autopilot"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "agentuse": "./bin/cli.js"
  },
  "files": [
    "bin/cli.js",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build:js": "bun build ./src/index.ts --outdir ./dist --target node --format esm --splitting --sourcemap --packages external",
    "build": "bun run clean && bun run build:js && chmod +x bin/cli.js",
    "watch": "bun build ./src/index.ts --outdir ./dist --target node --format esm --splitting --sourcemap --packages external --watch",
    "link": "npm link",
    "unlink": "npm unlink",
    "test": "bun test tests/compaction.test.ts tests/error-recovery.test.ts tests/parser.test.ts tests/bash-security.test.ts tests/bash-workdir.test.ts tests/edit-replacers.test.ts tests/path-validator.test.ts tests/command-validator.test.ts tests/integration-security.test.ts tests/add.test.ts && bun test tests/plugin-manager.test.ts && bun test tests/plugin-integration.test.ts tests/models.test.ts tests/subagent.test.ts tests/precedence.test.ts tests/skill-system.test.ts tests/devtools-integration.test.ts tests/spinner-cleanup.test.ts tests/learning-store.test.ts tests/learning-evaluator.test.ts tests/learning-extract.test.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build && bun run test",
    "pack-dry": "bun run build && npm pack --dry-run",
    "release": "np",
    "release:preview": "np --preview",
    "release:dry": "np --preview --any-branch",
    "docs:dev": "cd docs && mintlify dev",
    "docs:preview": "cd docs && mintlify preview",
    "generate:models": "npx tsx scripts/generate-models.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "3.0.1",
    "@ai-sdk/mcp": "1.0.1",
    "@ai-sdk/openai": "3.0.1",
    "@ai-sdk/provider": "^3.0.0",
    "@clack/prompts": "^0.11.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@openauthjs/openauth": "^0.4.3",
    "ai": "6.0.3",
    "chalk": "^5.5.0",
    "chokidar": "^5.0.0",
    "commander": "^12.0.0",
    "croner": "^9.1.0",
    "cronstrue": "^3.9.0",
    "dotenv": "^17.2.1",
    "esbuild-wasm": "0.23.1",
    "fuzzysort": "^3.1.0",
    "glob": "^11.0.3",
    "gray-matter": "^4.0.3",
    "minimatch": "^10.1.1",
    "ora": "^9.0.0",
    "posthog-node": "^5.18.0",
    "tree-sitter-bash": "^0.25.1",
    "ulid": "^3.0.1",
    "web-tree-sitter": "^0.26.3",
    "yaml": "^2.8.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@ai-sdk/devtools": "^0.0.1",
    "@types/bun": "^1.2.19",
    "np": "^10.2.0",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@10.11.1+sha512.e519b9f7639869dc8d5c3c5dfef73b3f091094b0a006d7317353c72b124e80e1afd429732e28705ad6bfa1ee879c1fce46c128ccebd3192101f43dd67c667912"
}
